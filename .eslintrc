{
  "extends": "airbnb",
  "rules": {
    "linebreak-style": ["error", "windows"],
    "indent": 0,
    "object-shorthand": 0,
    "quotes": ["error", "single"],
    "comma-dangle": ["error", "always-multiline"],
    "prefer-destructuring": ["error", {
      "array": false,
      "object": false
    }],
    "prefer-arrow-callback": 0,
    "no-undef": 0,
    "prefer-const": 0,
    "no-plusplus": 0,
    "no-mixed-operators": 0
  },
  "env": {
    "browser": true
  }
}